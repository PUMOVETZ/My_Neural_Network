    for (int i = 0; i < layers; i++) {
        std::cout << "Layer " << i + 1 << ":\n";
        for (int j = 0; j < network[i].size(); j++) {
            Neuron current_neuron = network[i][j];
            std::cout << "Neuron " << j + 1 << " in Layer " << i + 1 << ":\n";
            std::cout << "Weight: " << current_neuron.getWeight() << "\n";
            std::cout << "----------------\n";
        }
        std::cout << "\n";
    }
}